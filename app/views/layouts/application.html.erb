<!DOCTYPE html>
<html>
  <head>
    <title>SetList2003</title>
    <%= csrf_meta_tags %>

    <%= stylesheet_link_tag    'application', media: 'all' %>
    <%= javascript_include_tag 'application' %>
  </head>

  <body>
    <% flash.each do |type, message| %>
      <p>
        <%= message %>
      </p>
    <% end %>

    <p>Cart: <%= cart.total_count %> </p>

    <%= yield %>
  </body>
</html>
